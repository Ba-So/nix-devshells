{
  "mcpServers": {
    "codanna": {
      "type": "stdio",
      "command": "codanna",
      "args": ["serve", "--watch", "--watch-interval", "5"]
    },
    "serena": {
      "type": "stdio",
      "command": "serena",
      "args": ["start-mcp-server", "--transport", "stdio", "--project", "."],
      "env": {}
    },
    "claude-task-master": {
      "type": "stdio",
      "command": "task-master-mcp",
      "args": [],
      "env": {
        "TASKMASTER_DIR": ".taskmaster"
      }
    },
    "qdrant-docs": {
      "type": "stdio",
      "command": "mcp-server-qdrant",
      "args": [],
      "env": {
        "QDRANT_URL": "http://localhost:6333",
        "EMBEDDING_MODEL": "sentence-transformers/all-MiniLM-L6-v2"
      }
    }
  }
}
