{
  "mcpServers": {
    "shrimp": {
      "type": "stdio",
      "command": "mcp-shrimp-task-manager",
      "args": [],
      "env": {
        "DATA_DIR": ".shrimp",
        "TEMPLATES_USE": "en",
        "ENABLE_GUI": "true",
        "MCP_PROMPT_EXECUTE_TASK_APPEND": "Validation: Add that pre-commit hooks must pass and progress must be committed."
      }
    },
    "codanna": {
      "type": "stdio",
      "command": "codanna",
      "args": ["serve", "--watch", "--watch-interval", "5"]
    },
    "serena": {
      "type": "stdio",
      "command": "serena",
      "args": ["start-mcp-server", "--transport", "stdio", "--project", "."],
      "env": {}
    },
    "gitlab": {
      "type": "stdio",
      "command": "mcp-gitlab",
      "args": [],
      "env": {
        "GITLAB_PERSONAL_ACCESS_TOKEN": "",
        "GITLAB_API_URL": "https://gitlab.com/api/v4"
      }
    }
  }
}
